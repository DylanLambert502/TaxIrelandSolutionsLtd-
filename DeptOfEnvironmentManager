import java.util.ArrayList;

/**
 * Class to represent the Dept of Environment Mangers who will be using the system
 */
public class DeptOfEnvironmentManager {
    private ArrayList<Owner> propertyOwners;

    public DeptOfEnvironmentManager(){
        this.propertyOwners = new ArrayList<Owner>();
    }

    public ArrayList<Owner> getPropertyOwners() {
        return propertyOwners;
    }

    public void addOwner( Owner owner){
        propertyOwners.add(owner);
    }
}
